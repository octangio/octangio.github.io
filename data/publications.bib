  doi      = {10.1364/boe.8.001306},
  file     = {2017_Zang-et-al._Biomedical-Optics-Express_Automated-boundary-detection-of-the-optic-disc-and-layer-segmentation-of-the-peripapillary-re.pdf},
  issn     = {2156-7085},
  journal  = {Biomedical Optics Express},
