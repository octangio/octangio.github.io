  file     = {2016_Ang-et-al._Biomedical-Optics-Express_Automated-motion-correction-using-parallel--strip-registration-for-wide-field-en-face-OCT-angi.pdf},
  issn     = {21567085},
  journal  = {Biomedical Optics Express},
