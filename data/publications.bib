  file     = {2014_Jia-et-al._Ophthalmology_Quantitative-optical-coherence-tomography-angiography-of-choroidal-neovascularization-in-age-related-macul.pdf},
  isbn     = {0161-6420},
  issn     = {15494713},
  journal  = {Ophthalmology},
  number   = {7},
  pages    = {1435--1444},
  pmid     = {24679442},
  title    = {{Quantitative optical coherence tomography angiography of choroidal neovascularization in age-related macular degeneration}},
  volume   = {121},
  year     = {2014}
}
