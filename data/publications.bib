  number   = {12},
  pages    = {6412},
  title    = {{Maximum value projection produces better en face OCT angiograms than mean value projection}},
  volume   = {9},
  year     = {2018}
}
@article{Wang2019a,
  abstract = {{\textcopyright} 2018 Optical Society of America. The field of view of optical coherence tomography angiography (OCTA) images of the retina can be increased by montaging consecutive scans acquired at different retinal regions. Given the dramatic variation in aberrations throughout the entire posterior pole region, it is challenging to achieve seamless merging with apparent capillary continuity across the boundaries between adjacent angiograms. For this purpose, we propose herein a method that performs automated registration of contiguous OCTA images based on invariant features and uses a novel montage algorithm. The invariant features were used to register the overlapping areas between adjacently located scans by estimating the affine transformation matrix needed to accurately stitch them. Then, the flow signal was compensated to homogenize the angiograms with different brightness and the joints were blended to generate a seamless, montaged wide-field angiogram. We evaluated the algorithm on normal and diabetic retinopathy eyes. The proposed method could montage the angiograms seamlessly and provided a wide-field of view of retinal vasculature.},
  author   = {Wang, Jie and Camino, Acner and Hua, Xiaohui and Liu, Liang and Huang, David and Hwang, Thomas S. and Jia, Yali},
  doi      = {10.1364/boe.10.000120},
  file     = {2019_Wang-et-al._Biomedical-Optics-Express_Invariant-features-based-automated-registration-and-montage-for-wide-field-OCT-angiography.pdf},
  issn     = {2156-7085},
  journal  = {Biomedical Optics Express},
  number   = {1},
  pages    = {120},
  title    = {{Invariant features-based automated registration and montage for wide-field OCT angiography}},
  url      = {https://doi.org/10.1364/BOE.10.000120},
  volume   = {10},
  year     = {2019}
}
