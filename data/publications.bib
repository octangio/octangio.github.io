  file     = {2018_Wei-et-al._Optics-Letters_Fast-and-robust-standard-deviation-based-method-for-bulk-motion-compensation-in-phase-based-functional-OC.pdf},
  issn     = {0146-9592},
  journal  = {Optics Letters},
  number   = {9},
